user {{ nginx_user }};
worker_processes auto;

events {
    worker_connections 1024;
}

http {
    access_log {{ nginx_access_log }};
    error_log {{ nginx_error_log }};

    server {
        listen {{ nginx_listen_port }};
        server_name {{ nginx_server_name }};

        root {{ nginx_web_root }};
        index index.html;
    }
}
